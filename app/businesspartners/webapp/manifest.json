{
  "_version": "1.59.0",
  "sap.app": {
    "id": "businesspartners",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.12.3",
      "toolsId": "5b0f9d5b-d80e-407c-9c44-b5a70453c763"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/nnrg/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.8",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "businesspartners.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "BusinessPartnersList",
          "target": "BusinessPartnersList"
        },
        {
          "pattern": "BusinessPartners({key}):?query:",
          "name": "BusinessPartnersObjectPage",
          "target": "BusinessPartnersObjectPage"
        }
      ],
      "targets": {
        "BusinessPartnersList": {
          "type": "Component",
          "id": "BusinessPartnersList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "intialLoad": "Enabled",
              "contextPath": "/BusinessPartners",
              "variantManagement": "Page",
              "navigation": {
                "BusinessPartners": {
                  "detail": {
                    "route": "BusinessPartnersObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "actions": {
                    "IsVendor": {
                      "press": "businesspartners.ext.isVendor.IsVendor.IsVendor",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": true,
                      "text": "IsVendor"
                    },
                    "IsCustomer": {
                      "press": "businesspartners.ext.isCustomer.IsCustomer.IsCustomer",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": true,
                      "text": "IsCustomer"
                    },
                    "IsNotCustomer": {
                      "press": "businesspartners.ext.isNotCustomer.IsNotCustomer.IsNotCustomer",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": true,
                      "text": "IsNotCustomer"
                    },
                    "IsNotVendor": {
                      "press": "businesspartners.ext.isNotVendor.IsNotVendor.IsNotVendor",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": true,
                      "text": "IsNotVendor"
                    }
                  }
                }
              }
            }
          }
        },
        "BusinessPartnersObjectPage": {
          "type": "Component",
          "id": "BusinessPartnersObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "intialLoad": "Enabled",
              "editableHeaderContent": false,
              "contextPath": "/BusinessPartners"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
